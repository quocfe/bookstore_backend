{
	"version": 2,
	"builds": [
		{
			"src": "src/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/index.js"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "https://bookstore-frontend-wheat.vercel.app"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET, POST, PUT, DELETE"
				},
				{
					"key": "Access-Control-Allow-Credentials",
					"value": "true"
				}
			]
		}
	]
}
